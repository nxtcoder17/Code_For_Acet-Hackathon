<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:controller="college.roomsdb.Roomsdb"
      xmlns:fx="https://javafx.com/fxml"
      stylesheets="/college/themes/theme.css"
      alignment="CENTER"
      styleClass="rootPane" spacing="30" >

    <HBox spacing="30" alignment="CENTER">
        <Label text="Room No." styleClass="labels" alignment="CENTER"/>
        <TextField fx:id="field_room" styleClass="textField" prefWidth="250" />
    </HBox>

    <HBox spacing="50" alignment="CENTER">
        <Button fx:id="button_submit" styleClass="submitButton"
                text="Add Room" onAction="#button_submit_pressed" />
        <Button fx:id="button_cancel" styleClass="submitButton"
                text="Go Back" onAction="#button_cancel_pressed" />
    </HBox>
</VBox>

